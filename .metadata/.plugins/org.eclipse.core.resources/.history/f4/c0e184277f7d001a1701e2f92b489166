		package in.fantasticfour.onlinebookstore.entity;

import java.util.Collections;


public class temp {
		
			public static void main(String[] args) {
				// TODO Auto-generated method stub
		
			}
			
			int size;
		    public static int minPartitions(List<Integer> used, List<Integer> total) {
		    // Write your code here
		    total = Collections.sort(total);
		    int count = 0;
		    int usedSpace = 0;
		    int leftspace = 0;
		    for(int index :used){
		        usedSpace = usedSpace+index;
		    }

		    for(int index : total){
		     leftspace = usedSpace - index;
		     if(leftspace != 0){
		         count++;
		     }
		     
		     }
		    
		    return count;
		    }

		
		}
